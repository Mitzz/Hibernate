<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.mitz.model">
	<class name="Item" table="ITEM_MASTER">
		<id name="id" column="ITEM_ID" type="java.lang.Long">
			<generator class="native" />
		</id>
		<property name="name" type="java.lang.String" column="NAME"/>
		<set name="images" table="ITEM_IMAGE" sort="natural" lazy="false">
			<key column="ITEM_ID" />
			<element column="IMAGE_NAME" type="java.lang.String" />
		</set>
	</class>
</hibernate-mapping>